{
  "name": "dwn-cli",
  "description": "A simple CLI to interact with the Decentralized Web Nodes (DWN) APIs.",
  "version": "0.0.0",
  "author": "Decentralized Identity Foundation",
  "license": "Apache-2.0",
  "repository": "https://github.com/decentralized-identity/dwn-cli-sample.git",
  "homepage": "https://github.com/decentralized-identity/dwn-cli-app",
  "bugs": "https://github.com/decentralized-identity/dwn-cli-app/issues",
  "keywords": [
    "DWN"
  ],
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "types": "dist/index.d.ts",
  "pkg": {
    "scripts": "./dist/**/*.js"
  },
  "bin": {
    "dwn-cli": "./bin/run.js"
  },
  "oclif": {
    "bin": "dwn-cli",
    "dirname": "dwn-cli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " "
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "@types/readable-stream": "^4.0.14",
    "@web5/api": "0.0.0-next-8222902-20240718015236",
    "pkg": "^5.8.1"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
